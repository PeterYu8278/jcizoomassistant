# Gemini AI API Key (for meeting agenda generation)
GEMINI_API_KEY=your_gemini_api_key_here

# Zoom API (Server-to-Server OAuth - create at marketplace.zoom.us)
VITE_ZOOM_ACCOUNT_ID=your_account_id_here
VITE_ZOOM_CLIENT_ID=your_client_id_here
VITE_ZOOM_CLIENT_SECRET=your_client_secret_here

# Enable/Disable Zoom API integration
# Set to 'true' to use real Zoom API, 'false' to use mock links
VITE_USE_ZOOM_API=false
