# Gemini AI API Key (for meeting agenda generation)
GEMINI_API_KEY=your_gemini_api_key_here

# Zoom API Configuration
# Get your credentials from: https://marketplace.zoom.us/develop/create
VITE_ZOOM_API_KEY=your_zoom_api_key_here
VITE_ZOOM_API_SECRET=your_zoom_api_secret_here
VITE_ZOOM_ACCOUNT_ID=your_zoom_account_id_here

# Enable/Disable Zoom API integration
# Set to 'true' to use real Zoom API, 'false' to use mock links
VITE_USE_ZOOM_API=false
